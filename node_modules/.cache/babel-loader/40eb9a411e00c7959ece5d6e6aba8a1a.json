  <v-progress-linear\r\n          absolute\r\n          color=\"green lighten-3\"\r\n          height=\"4\"\r\n          indeterminate\r\n      ></v-progress-linear>\r\n    </template>\r\n    <v-img\r\n        height=\"300\"\r\n\r\n        src=\"https://cdn.vuetifyjs.com/images/cards/dark-beach.jpg\"\r\n    >\r\n\r\n    </v-img>\r\n    <v-form>\r\n      <v-container id=\"text\">\r\n        <div style=\"width: 280px\">\r\n\r\n          <v-text-field\r\n              v-model=\"pas\"\r\n              :disabled=\"isUpdating\"\r\n              class=\"text\"\r\n              filled\r\n              color=\"blue-grey lighten-2\"\r\n              label=\"用户名\"\r\n          ></v-text-field>\r\n        </div>\r\n        <div style=\"width: 280px\">\r\n\r\n          <v-text-field\r\n              v-model=\"names\"\r\n              :disabled=\"isUpdating\"\r\n              filled\r\n              class=\"text\"\r\n              type=\"password\"\r\n              color=\"blue-grey lighten-2\"\r\n              label=\"密码\"\r\n          ></v-text-field>\r\n        </div>\r\n      </v-container>\r\n    </v-form>\r\n    <v-divider></v-divider>\r\n    <v-card-actions>\r\n\r\n      <v-spacer></v-spacer>\r\n      <v-btn\r\n          :loading=\"isUpdating\"\r\n          color=\"blue-grey darken-3\"\r\n          depressed\r\n          @click=\"submit()\"\r\n      >\r\n        <v-icon left>\r\n          mdi-update\r\n        </v-icon>\r\n        登录\r\n      </v-btn>\r\n    </v-card-actions>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport {request} from \"../netwoek/request\";\r\n\r\nexport default {\r\n  name: \"Sign2\",\r\n  data(){\r\n    return{\r\n      names:'',\r\n      pas:'',\r\n      isUpdating: false,\r\n    }\r\n  },\r\n  methods:{\r\n    submit(){\r\n      this.isUpdating=true\r\n      request({\r\n        url:'/admin/login',\r\n        method:'post',\r\n        data:{\r\n          // \"password\": this.pas,\r\n          // \"username\": this.names\r\n          \"password\": '123456',\r\n          \"username\": 'admin'\r\n        },\r\n\r\n        //\r\n      })\r\n          .then(res=>{this.$store.commit(\"setToken\",res.data.token)\r\n            if (this.$store.state.token){\r\n              alert('登录成功')\r\n              this.$router.push('/Dashboard')\r\n            }\r\n          })\r\n          .catch(()=>{\r\n            this.isUpdating=true\r\n\r\n          })\r\n\r\n      this.$router.push('/Dashboard')\r\n\r\n\r\n\r\n\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#card{\r\n  top: 100px;\r\n  margin: auto;\r\n}\r\n#text{\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components/HomeItem"}]}