ACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,GAAA,CAAA,EAAA;AAEA,UAAA,MAAA,CAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA,CAAA;;AACA,UAAA,MAAA,CAAA,WAAA;AACA;AACA,OAPA,EAQA,KARA,CAQA,YAAA;AAAA,QAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,MAAA,GAAA,IAAA;AAAA,QAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,IAAA,GAAA,kBAAA;AAAA,OARA;AAaA,KApFA;AAqFA,IAAA,QArFA,sBAqFA;AACA,UAAA,KAAA,IAAA,GAAA,CAAA,IAAA,KAAA,aAAA,EAAA,KAAA,IAAA,IAAA,CAAA;AACA,KAvFA;AAwFA,IAAA,UAxFA,wBAwFA;AACA,UAAA,KAAA,IAAA,GAAA,CAAA,IAAA,CAAA,EAAA,KAAA,IAAA,IAAA,CAAA;AACA,KA1FA;AA2FA,IAAA,kBA3FA,8BA2FA,MA3FA,EA2FA;AACA,WAAA,YAAA,GAAA,MAAA;AACA,KA7FA,CA+FA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAzGA,GAjGA;AA6MA,EAAA,KAAA,EAAA;AACA,IAAA,MADA,kBACA,GADA,EACA;AACA,MAAA,GAAA,IAAA,KAAA,KAAA,EAAA;AACA,KAHA;AAIA,IAAA,YAJA,wBAIA,GAJA,EAIA;AACA,MAAA,GAAA,IAAA,KAAA,WAAA,EAAA;AACA;AANA,GA7MA;AAqNA,EAAA,UAAA,EAAA;AACA,IAAA,WAAA,EAAA;AADA;AArNA,CAAA","sourcesContent":["<template>\r\n  <v-container fluid>\r\n\r\n    <v-dialog\r\n        v-model=\"dialog\"\r\n        max-width=\"500px\"\r\n    >\r\n\r\n\r\n      <v-card>\r\n        <v-card-title>\r\n          <span class=\"headline\">{{ formTitle }}</span>\r\n        </v-card-title>\r\n\r\n        <v-card-text>\r\n          <v-container>\r\n            <v-row>\r\n\r\n              <v-col\r\n                  cols=\"12\"\r\n                  sm=\"6\"\r\n                  md=\"4\"\r\n              >\r\n                <v-text-field\r\n                    v-model=\"editedItem.sid\"\r\n                    label=\"学号\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col\r\n                  cols=\"12\"\r\n                  sm=\"6\"\r\n                  md=\"4\"\r\n              >\r\n                <v-text-field\r\n                    v-model=\"editedItem.name\"\r\n                    label=\"姓名\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col\r\n                  cols=\"12\"\r\n                  sm=\"6\"\r\n                  md=\"4\"\r\n              >\r\n<!--                <template>-->\r\n<!--                  <v-file-input-->\r\n<!--                      show-size-->\r\n<!--                      accept=\"image/*\"-->\r\n<!--                      label=\"照片\"-->\r\n<!--                      v-model=\"editedItem.image\"-->\r\n<!--                  ></v-file-input>-->\r\n<!--                </template>-->\r\n              </v-col>\r\n              <v-textarea\r\n                  v-model=\"editedItem.remarks\"\r\n                  label=\"请添加描述\"\r\n              ></v-textarea>\r\n            </v-row>\r\n          </v-container>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click=\"close\"\r\n          >\r\n            取消\r\n          </v-btn>\r\n          <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click=\"save\"\r\n          >\r\n            保存\r\n          </v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n      <v-card>\r\n        <v-card-title class=\"headline\">确定要删除这个选项吗</v-card-title>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\">确定</v-btn>\r\n          <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">取消</v-btn>\r\n          <v-spacer></v-spacer>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n    <failDialogs  ref=\"failDialogs\"></failDialogs>\r\n    <v-data-iterator\r\n        :items=\"items\"\r\n        :items-per-page.sync=\"itemsPerPage\"\r\n        :page.sync=\"page\"\r\n        :search=\"search\"\r\n        :sort-by=\"sortBy.toLowerCase()\"\r\n        :sort-desc=\"sortDesc\"\r\n        hide-default-footer\r\n        no-data-text=\"未查询到数据\"\r\n\r\n    >\r\n\r\n      <template v-slot:header>\r\n        <v-toolbar\r\n            dark\r\n            color=\"blue darken-3\"\r\n            class=\"mb-1\"\r\n        >\r\n          <v-text-field\r\n              v-model=\"search\"\r\n              clearable\r\n              flat\r\n              solo-inverted\r\n              hide-details\r\n              prepend-inner-icon=\"mdi-magnify\"\r\n              label=\"搜索\"\r\n          ></v-text-field>\r\n            <v-btn\r\n                large\r\n                depressed\r\n                color=\"blue\"\r\n                style=\"margin: 5px\"\r\n                @click=\"editItem()\"\r\n            >\r\n              添加学生\r\n            </v-btn>\r\n          <template v-if=\"$vuetify.breakpoint.mdAndUp\">\r\n\r\n            <v-btn-toggle\r\n                v-model=\"sortDesc\"\r\n                mandatory\r\n            >\r\n              <v-btn\r\n                  large\r\n                  depressed\r\n                  color=\"blue\"\r\n                  :value=\"false\"\r\n              >\r\n                <v-icon>mdi-arrow-up</v-icon>\r\n              </v-btn>\r\n              <v-btn\r\n                  large\r\n                  depressed\r\n                  color=\"blue\"\r\n                  :value=\"true\"\r\n              >\r\n                <v-icon>mdi-arrow-down</v-icon>\r\n              </v-btn>\r\n            </v-btn-toggle>\r\n          </template>\r\n        </v-toolbar>\r\n      </template>\r\n\r\n      <template v-slot:default=\"props\">\r\n        <v-row>\r\n          <v-col\r\n              v-for=\"item in props.items\"\r\n              :key=\"item.sid\"\r\n              cols=\"12\"\r\n              sm=\"6\"\r\n              md=\"4\"\r\n              lg=\"3\"\r\n          >\r\n            <v-card>\r\n\r\n              <v-card-title class=\"subheading font-weight-bold\">\r\n<!--                <img :src=\"item.image\" width=\"150px\" height=\"200px\" >-->\r\n\r\n\r\n\r\n              </v-card-title>\r\n\r\n              <v-divider></v-divider>\r\n              学生姓名:{{ item.name }}\r\n\r\n              <v-icon\r\n                  small\r\n                  class=\"mr-2\"\r\n                  @click=\"editItem(item)\"\r\n                  color=\"green\"\r\n              >\r\n                mdi-pencil\r\n              </v-icon>\r\n              <v-icon\r\n                  small\r\n                  @click=\"deleteItem(item)\"\r\n                  color=\"red\"\r\n              >\r\n                mdi-delete\r\n              </v-icon>\r\n              <v-list dense>\r\n\r\n                <v-list-item\r\n                    v-for=\"(key, index) in filteredKeys\"\r\n                    :key=\"index\"\r\n                >\r\n\r\n                  <v-list-item-content :class=\"{ 'blue--text': sortBy === key }\">\r\n                    {{ key }}:\r\n                  </v-list-item-content>\r\n                  <v-list-item-content\r\n                      class=\"align-end\"\r\n                      :class=\"{ 'blue--text': sortBy === key }\"\r\n                  >\r\n                    {{ item[key.toLowerCase()] }}\r\n                  </v-list-item-content>\r\n                </v-list-item>\r\n              </v-list>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </template>\r\n\r\n      <template v-slot:footer>\r\n        <v-row\r\n            class=\"mt-2\"\r\n            align=\"center\"\r\n            justify=\"center\"\r\n        >\r\n          <span class=\"grey--text\">Items per page</span>\r\n          <v-menu offset-y>\r\n            <template v-slot:activator=\"{ on, attrs }\">\r\n              <v-btn\r\n                  dark\r\n                  text\r\n                  color=\"primary\"\r\n                  class=\"ml-2\"\r\n                  v-bind=\"attrs\"\r\n                  v-on=\"on\"\r\n              >\r\n                {{ itemsPerPage }}\r\n                <v-icon>mdi-chevron-down</v-icon>\r\n              </v-btn>\r\n            </template>\r\n            <v-list>\r\n              <v-list-item\r\n                  v-for=\"(number, index) in itemsPerPageArray\"\r\n                  :key=\"index\"\r\n                  @click=\"updateItemsPerPage(number)\"\r\n              >\r\n                <v-list-item-title>{{ number }}</v-list-item-title>\r\n              </v-list-item>\r\n            </v-list>\r\n          </v-menu>\r\n\r\n          <v-spacer></v-spacer>\r\n\r\n          <span\r\n              class=\"mr-4\r\n            grey--text\"\r\n          >\r\n            Page {{ page }} of {{ numberOfPages }}\r\n          </span>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"mr-1\"\r\n              @click=\"formerPage\"\r\n          >\r\n            <v-icon>mdi-chevron-left</v-icon>\r\n          </v-btn>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"ml-1\"\r\n              @click=\"nextPage\"\r\n          >\r\n            <v-icon>mdi-chevron-right</v-icon>\r\n          </v-btn>\r\n        </v-row>\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport failDialogs from \"./failDialogs\";\r\nimport {addStuListFromApi, deleteStuListFromApi, getStuFromApi, updateStuListFromApi} from \"../../network/stuApi\";\r\nimport {ifthen} from \"../../network/FN\";\r\n\r\nexport default {\r\n  name:'TableStu',\r\n  data () {\r\n    return {\r\n      addItem:{},\r\n      itemsPerPageArray: [4, 8, 12],\r\n      search: '',\r\n      filter: {},\r\n      sortDesc: false,\r\n      page: 1,\r\n      itemsPerPage: 8,\r\n      sortBy: 'name',\r\n      editedIndex: -1,\r\n      dialog: false,\r\n      dialogDelete:false,\r\n      editedItem: {\r\n        name: null,\r\n        sid: null,\r\n        remarks: null,\r\n       // image:null\r\n      },\r\n      defaultItem: {\r\n        name: null,\r\n        sid: null,\r\n        remarks: null,\r\n      //  image:null\r\n\r\n      },\r\n      keys: [\r\n        'Name',\r\n        'Sid',\r\n        'Remarks',\r\n\r\n      ],\r\n      items: [\r\n        {\r\n          name:'小明',\r\n          sid:'194105401',\r\n          remarks:'班长'\r\n        },\r\n        {\r\n          name:'小明',\r\n          sid:'194105402',\r\n          remarks:'学委'\r\n        },\r\n        {\r\n          name:'小明',\r\n          sid:'194105403',\r\n          remarks:'体委'\r\n        },\r\n        {\r\n          name:'小明',\r\n          sid:'194105404',\r\n          remarks:''\r\n        },\r\n        {\r\n          name:'小明',\r\n          sid:'194105405',\r\n          remarks:''\r\n        },\r\n      ],\r\n    }\r\n  },\r\n  props:{\r\n    pid:{\r\n      type:String\r\n    }\r\n  },\r\n  created() {\r\n    getStuFromApi(this.pid)\r\n        .then(res=>{\r\n          if(ifthen(res)){\r\n\r\n          this.items=[]\r\n          for(let i=0;i<res.data.list.length;i++) {\r\n            this.addItem['name']= res.data.list[i].name\r\n            this.addItem['sid']= res.data.list[i].sid\r\n            this.addItem['remarks']= res.data.list[i].remarks\r\n\r\n            this.items.push(this.addItem);\r\n            this.addItem=[]\r\n          }\r\n          }\r\n        })\r\n        .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='获取学生信息失败请重新尝试或检查网络连接'})\r\n  },\r\n  computed: {\r\n    formTitle () {\r\n      return this.editedIndex === -1 ? '新增学生信息' : '编辑学生信息'\r\n    },\r\n    numberOfPages () {\r\n      return Math.ceil(this.items.length / this.itemsPerPage)\r\n    },\r\n    filteredKeys () {\r\n      return this.keys.filter(key => key !== 'Name')\r\n    },\r\n  },\r\n  methods: {\r\n    editItem (item) {\r\n      this.editedIndex = this.items.indexOf(item)\r\n      this.editedItem = Object.assign({}, item)\r\n      this.dialog = true\r\n    },\r\n    close () {\r\n      this.dialog = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    save () {\r\n      if (this.editedIndex > -1) {\r\n        console.log('修改');\r\n\r\n        if(this.editedItem.name&&this.editedItem.sid&&this.editedItem.remarks){\r\n          Object.assign(this.items[this.editedIndex], this.editedItem)\r\n          this.close()\r\n          updateStuListFromApi(this.editedItem,this.pid)\r\n            .then((res)=>{\r\n              if(ifthen(res)) {\r\n                Object.assign(this.items[this.editedIndex], this.editedItem)\r\n                this.close()\r\n              }\r\n            })\r\n              .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='修改失败请重新尝试或检查网络连接'})\r\n\r\n        }else{\r\n          alert('请输入完整')\r\n        }\r\n      } else {\r\n        console.log('添加');\r\n\r\n        if(this.editedItem.sid&&this.editedItem.name&&this.editedItem.remarks){\r\n         //this.imageToBase64(this.editedItem.image)\r\n          this.items.push(this.editedItem)\r\n          this.close()\r\n          addStuListFromApi(this.editedItem,this.pid)\r\n            .then((res)=>{\r\n              if(ifthen(res)) {\r\n                this.items.push(this.editedItem)\r\n                this.close()\r\n              }\r\n            })\r\n            .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='添加失败请重新尝试或检查网络连接'})\r\n\r\n\r\n        }else{\r\n          alert('请输入完整')\r\n        }\r\n      }\r\n    },\r\n    closeDelete () {\r\n      this.dialogDelete = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    deleteItem (item) {\r\n      this.editedIndex = this.items.indexOf(item)\r\n      this.editedItem = Object.assign({}, item)\r\n      this.dialogDelete = true\r\n      this.editedItem=item\r\n    },\r\n\r\n    deleteItemConfirm () {\r\n      this.items.splice(this.editedIndex, 1)\r\n      this.closeDelete()\r\n      deleteStuListFromApi(this.editedItem)\r\n        .then((res)=>{\r\n          if(ifthen(res)) {\r\n\r\n            this.items.splice(this.editedIndex, 1)\r\n            this.closeDelete()\r\n          }\r\n        })\r\n          .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='删除失败请重新尝试或检查网络连接'})\r\n\r\n\r\n\r\n\r\n    },\r\n    nextPage () {\r\n      if (this.page + 1 <= this.numberOfPages) this.page += 1\r\n    },\r\n    formerPage () {\r\n      if (this.page - 1 >= 1) this.page -= 1\r\n    },\r\n    updateItemsPerPage (number) {\r\n      this.itemsPerPage = number\r\n    },\r\n\r\n    //  imageToBase64 (file) {\r\n    //   let reader = new FileReader()\r\n    //   reader.readAsDataURL(file)\r\n    //   reader.onload = () => {\r\n    //     console.log('file 转 base64结果：' + reader.result)\r\n    //     this.editedItem.image=reader.result\r\n    //   }\r\n    //   reader.onerror = function (error) {\r\n    //     console.log('Error: ', error)\r\n    //   }\r\n    // }\r\n\r\n  },\r\n  watch:{\r\n    dialog (val) {\r\n      val || this.close()\r\n    },\r\n    dialogDelete (val) {\r\n      val || this.closeDelete()\r\n    },\r\n  },\r\n  components:{\r\n    failDialogs\r\n  }\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/components/HomeItem"}]}