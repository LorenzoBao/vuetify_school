AA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,MAAA,IAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,IAAA;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,KAAA,IAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,CAAA,EAAA,GAAA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,OAAA,GAAA,EAAA;AACA;AACA;AACA,OAXA,EAYA,KAZA,CAYA,YAAA;AAAA,QAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,MAAA,GAAA,IAAA;AAAA,QAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,IAAA,GAAA,sBAAA;AAAA,OAZA;AAaA,KAfA;AAiBA,IAAA,MAjBA,kBAiBA,GAjBA,EAiBA;AACA,MAAA,GAAA,IAAA,KAAA,KAAA,EAAA;AACA,KAnBA;AAoBA,IAAA,YApBA,wBAoBA,GApBA,EAoBA;AACA,MAAA,GAAA,IAAA,KAAA,WAAA,EAAA;AACA;AAtBA;AA/JA,CAAA","sourcesContent":["<template>\r\n  <v-container fluid >\r\n    <v-dialog\r\n        v-model=\"dialog\"\r\n        max-width=\"500px\"\r\n    >\r\n      <failDialogs ref=\"failDialogs\"></failDialogs>\r\n\r\n\r\n      <v-card>\r\n        <v-card-title >\r\n          <span class=\"headline\">{{ formTitle }}</span>\r\n        </v-card-title>\r\n\r\n        <v-card-text>\r\n          <v-container>\r\n            <v-row>\r\n\r\n              <v-col\r\n                  cols=\"12\"\r\n                  sm=\"6\"\r\n                  md=\"4\"\r\n              >\r\n                <v-text-field\r\n                    v-model=\"editedItem.pid\"\r\n                    label=\"班级ID\"\r\n                ></v-text-field>\r\n              </v-col>\r\n              <v-col\r\n                  cols=\"16\"\r\n                  sm=\"10\"\r\n                  md=\"8\"\r\n              >\r\n                <v-text-field\r\n                    v-model=\"editedItem.name\"\r\n                    label=\"班级名\"\r\n                ></v-text-field>\r\n              </v-col>\r\n\r\n            </v-row>\r\n          </v-container>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click=\"close\"\r\n          >\r\n            取消\r\n          </v-btn>\r\n          <v-btn\r\n              color=\"blue darken-1\"\r\n              text\r\n              @click=\"save\"\r\n          >\r\n            保存\r\n          </v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\r\n      <v-card>\r\n        <v-card-title class=\"headline\">确定要删除这个选项吗</v-card-title>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\">确定</v-btn>\r\n          <v-btn color=\"blue darken-1\" text @click=\"closeDelete\">取消</v-btn>\r\n          <v-spacer></v-spacer>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n\r\n    <v-data-iterator\r\n        :item-key=\"cid\"\r\n        :items=\"items\"\r\n        :items-per-page.sync=\"itemsPerPage\"\r\n        :page.sync=\"page\"\r\n        :search=\"search\"\r\n        :sort-by=\"sortBy.toLowerCase()\"\r\n        no-data-text=\"未查询到数据\"\r\n        hide-default-footer\r\n\r\n    >\r\n      <template v-slot:header>\r\n        <v-toolbar\r\n            dark\r\n            color=\"blue darken-3\"\r\n            class=\"mb-1\"\r\n        >\r\n          <v-text-field\r\n              v-model=\"search\"\r\n              clearable\r\n              flat\r\n              solo-inverted\r\n              hide-details\r\n              prepend-inner-icon=\"mdi-magnify\"\r\n              label=\"搜索\"\r\n          ></v-text-field>\r\n          <v-btn\r\n              large\r\n              depressed\r\n              color=\"blue\"\r\n              style=\"margin: 5px\"\r\n              @click=\"editItem()\"\r\n          >\r\n            添加班级\r\n          </v-btn>\r\n        </v-toolbar>\r\n      </template>\r\n\r\n      <template v-slot:default=\"props\">\r\n        <v-row>\r\n          <v-col\r\n              v-for=\"item in props.items\"\r\n              :key=\"item.name\"\r\n              cols=\"12\"\r\n              sm=\"6\"\r\n              md=\"4\"\r\n              lg=\"3\"\r\n          >\r\n            <v-card  class=\"card\"    @click=\"goClass(item.pid)\" >\r\n              <v-card-title class=\"subheading font-weight-bold title\">\r\n                <span>{{ item.name }}</span>\r\n\r\n                <v-icon\r\n\r\n                    small\r\n                    class=\"mr-2 btn\"\r\n                    @click.stop=\"editItem(item)\"\r\n                    color=\"green\"\r\n                >\r\n                  mdi-pencil\r\n                </v-icon>\r\n                <v-icon\r\n                    class=\"btn\"\r\n                    small\r\n                    @click.stop=\"deleteItem(item)\"\r\n                    color=\"red\"\r\n                >\r\n                  mdi-delete\r\n                </v-icon>\r\n              </v-card-title>\r\n\r\n              <v-divider></v-divider>\r\n\r\n              <v-list dense>\r\n                <v-list-item\r\n                    v-for=\"(key, index) in filteredKeys\"\r\n                    :key=\"index\"\r\n                >\r\n                  <v-list-item-content :class=\"{ 'blue--text': sortBy === key }\">\r\n                    {{ key }}:\r\n                  </v-list-item-content>\r\n                  <v-list-item-content\r\n                      class=\"align-end\"\r\n                      :class=\"{ 'blue--text': sortBy === key }\"\r\n                  >\r\n                    {{ item[key.toLowerCase()] }}\r\n                  </v-list-item-content>\r\n                </v-list-item>\r\n              </v-list>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </template>\r\n\r\n      <template v-slot:footer>\r\n        <v-row\r\n            class=\"mt-2\"\r\n            align=\"center\"\r\n            justify=\"center\"\r\n        >\r\n\r\n\r\n          <v-spacer></v-spacer>\r\n\r\n          <span\r\n              class=\"mr-4\r\n            grey--text\"\r\n          >\r\n            第 {{ page }} 到 {{ numberOfPages }}\r\n          </span>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"mr-1\"\r\n              @click=\"formerPage\"\r\n          >\r\n            <v-icon>mdi-chevron-left</v-icon>\r\n          </v-btn>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"ml-1\"\r\n              @click=\"nextPage\"\r\n          >\r\n            <v-icon>mdi-chevron-right</v-icon>\r\n          </v-btn>\r\n        </v-row>\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport failDialogs from \"./failDialogs\";\r\nimport {\r\n  addClassListFromApi,\r\n  deleteClassListFromApi,\r\n  getClassFromApi,\r\n  updateClassListFromApi\r\n} from \"../../network/classApi\";\r\nimport {ifthen} from \"../../network/FN\";\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      search: '',\r\n      filter: {},\r\n      addItem:{},\r\n      page: 1,\r\n      itemsPerPage: 12,\r\n      sortBy: 'name',\r\n      editedIndex: -1,\r\n      dialog: false,\r\n      dialogDelete:false,\r\n      editedItem: {\r\n        name: null,\r\n        pid: null,\r\n      },\r\n      defaultItem: {\r\n        name: null,\r\n        pid: null,\r\n      },\r\n      keys: [\r\n        'Name',\r\n        'Pid',\r\n\r\n      ],\r\n      items: [\r\n        {\r\n        }\r\n      ],\r\n    }\r\n  },\r\n  props:{\r\n    ClassName:{\r\n      type:Array,\r\n\r\n    }\r\n  },\r\n  created() {\r\n    getClassFromApi(this.ClassName.cid)\r\n        .then(res=>{\r\n          if(ifthen(res)) {\r\n\r\n            this.items = []\r\n            for (let i = 0; i < res.data.list.length; i++) {\r\n              this.addItem['name'] = res.data.list[i].name\r\n              this.addItem['pid'] = res.data.list[i].pid\r\n              this.items.push(this.addItem);\r\n              this.addItem = []\r\n            }\r\n          }\r\n        })\r\n        .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='获取班级信息失败请重新尝试或检查网络连接'})\r\n  },\r\n  computed: {\r\n    cid(){\r\n      return this.ClassName.cid+''\r\n    },\r\n    formTitle () {\r\n      return this.editedIndex === -1 ? '新增学生信息' : '编辑学生信息'\r\n    },\r\n    numberOfPages () {\r\n      return Math.ceil(this.items.length / this.itemsPerPage)\r\n    },\r\n    filteredKeys () {\r\n      return this.keys.filter(key => key !== 'Name')\r\n    },\r\n  },\r\n  methods: {\r\n    editItem (item) {\r\n      this.editedIndex = this.items.indexOf(item)\r\n      this.editedItem = Object.assign({}, item)\r\n      this.dialog = true\r\n    },\r\n    close () {\r\n      this.dialog = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    save () {\r\n      if (this.editedIndex > -1) {\r\n        console.log('修改');\r\n        if(this.editedItem.name&&this.editedItem.pid){\r\n\r\n          updateClassListFromApi(this.editedItem,this.ClassName.cid)\r\n              .then((res)=>{\r\n                if(ifthen(res)) {\r\n                  this.$set(this.items,this.editedIndex,this.editedItem)\r\n                  this.close()\r\n                }\r\n              })\r\n              .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='修改失败请重新尝试或检查网络连接'})\r\n\r\n\r\n        }else{\r\n          alert('请输入完整')\r\n        }\r\n      } else {\r\n        console.log('添加');\r\n        if(this.editedItem.pid&&this.editedItem.name){\r\n\r\n          addClassListFromApi(this.editedItem,this.ClassName.cid)\r\n              .then((res)=>{\r\n                if(ifthen(res)) {\r\n                  this.items.push(this.editedItem)\r\n                  this.close()\r\n                }\r\n              })\r\n              .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='添加失败请重新尝试或检查网络连接'})\r\n\r\n\r\n        }else{\r\n          alert('请输入完整')\r\n        }\r\n      }\r\n    },\r\n    closeDelete () {\r\n      this.dialogDelete = false\r\n      this.$nextTick(() => {\r\n        this.editedItem = Object.assign({}, this.defaultItem)\r\n        this.editedIndex = -1\r\n      })\r\n    },\r\n    deleteItem (item) {\r\n\r\n      this.editedIndex = this.items.indexOf(item)\r\n      this.editedItem = Object.assign({}, item)\r\n      this.dialogDelete = true\r\n\r\n    },\r\n\r\n    deleteItemConfirm () {\r\n        deleteClassListFromApi(this.editedItem)\r\n          .then((res)=>{\r\n            if(ifthen(res)) {\r\n              this.items.splice(this.editedIndex, 1)\r\n              this.closeDelete()\r\n            }\r\n              })\r\n          .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='删除失败请重新尝试或检查网络连接'})\r\n\r\n\r\n\r\n    },\r\n    goClass(pid){\r\n      this.$router.push('/ClassCr/'+pid)\r\n    },\r\n    nextPage () {\r\n      if (this.page + 1 <= this.numberOfPages) this.page += 1\r\n    },\r\n    formerPage () {\r\n      if (this.page - 1 >= 1) this.page -= 1\r\n    },\r\n\r\n  },\r\n  components:{\r\n    failDialogs\r\n  },\r\n  watch:{\r\n    ClassName(val){\r\n      getClassFromApi(this.ClassName.cid)\r\n          .then(res=>{\r\n            if(ifthen(res)) {\r\n              this.items = []\r\n              for (let i = 0; i < res.data.list.length; i++) {\r\n                this.addItem['name'] = res.data.list[i].name\r\n                this.addItem['pid'] = res.data.list[i].pid\r\n                this.items.push(this.addItem);\r\n                this.addItem = []\r\n              }\r\n            }\r\n          })\r\n          .catch(()=>{this.$refs.failDialogs.dialog=true;this.$refs.failDialogs.text='获取班级信息失败请重新尝试或检查网络连接'})\r\n    },\r\n\r\n    dialog (val) {\r\n      val || this.close()\r\n    },\r\n    dialogDelete (val) {\r\n      val || this.closeDelete()\r\n    },\r\n  }\r\n}\r\n</script>\r\n<style scoped>\r\n.title{\r\n  min-height: 100px;\r\n  min-width: 285px;\r\n\r\n}\r\n .card:hover{\r\n  box-shadow: #c0ccda 7px 7px;\r\n}\r\n .btn:hover{\r\n   box-shadow: #c0ccda 2px 2px;\r\n\r\n }\r\n</style>"],"sourceRoot":"src/components/HomeItem"}]}