   return request({\r\n        url:'/admin/getUserList',\r\n        method:'get',\r\n    })\r\n}\r\nexport function updateAdminPasswordFromApi(username,oldPaw,newPaw) {\r\n    return request({\r\n        url:'/admin/revise',\r\n        method:'post',\r\n        data:{\r\n            username:username,\r\n            oldPassword:oldPaw,\r\n            newPassword:newPaw\r\n\r\n        }\r\n    })\r\n}\r\n// export function updateAdminInfoFromApi(items) {\r\n//     console.log(items);\r\n//     return request({\r\n//         url:'/admin/update',\r\n//         method:'post',\r\n//         data:{\r\n//             enable:1,\r\n//             pid:items.pid,\r\n//             remark:items.remark,\r\n//             type:items.type,\r\n//             username:items.username,\r\n//\r\n//\r\n//         }\r\n//     })\r\n// }\r\nexport function addImage(image,username) {\r\n    return request({\r\n\r\n        url:'/source/img/upload',\r\n        method:'post',\r\n        data:{\r\n            file:image,\r\n            username:username\r\n\r\n        }\r\n    })\r\n}\r\nexport function getImage(username) {\r\n    return request({\r\n\r\n        url:'/source/img/'+username,\r\n        method:'get',\r\n\r\n    })\r\n}\r\nexport function getAdminInfo() {\r\n    return request({\r\n\r\n        url:'/admin/info',\r\n        method:'get',\r\n\r\n    })\r\n}\r\nexport function reAdminPaw(username) {\r\n    return request({\r\n\r\n        url:'/admin/reset/'+username,\r\n        method:'post',\r\n\r\n    })\r\n}\r\n\r\n\r\n\r\n"]}]}