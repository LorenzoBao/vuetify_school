\n              v-model=\"search\"\r\n              clearable\r\n              flat\r\n              solo-inverted\r\n              hide-details\r\n              prepend-inner-icon=\"mdi-magnify\"\r\n              label=\"Search\"\r\n          ></v-text-field>\r\n\r\n        </v-toolbar>\r\n      </template>\r\n\r\n      <template v-slot:default=\"props\">\r\n        <v-row>\r\n          <v-col\r\n              v-for=\"item in props.items\"\r\n              :key=\"item.name\"\r\n              cols=\"12\"\r\n              sm=\"6\"\r\n              md=\"4\"\r\n              lg=\"3\"\r\n          >\r\n            <v-card     @click=\"goClass(item.pid)\">\r\n              <v-card-title class=\"subheading font-weight-bold\">\r\n                {{ item.name }}\r\n              </v-card-title>\r\n\r\n              <v-divider></v-divider>\r\n\r\n              <v-list dense>\r\n                <v-list-item\r\n                    v-for=\"(key, index) in filteredKeys\"\r\n                    :key=\"index\"\r\n                >\r\n                  <v-list-item-content :class=\"{ 'blue--text': sortBy === key }\">\r\n                    {{ key }}:\r\n                  </v-list-item-content>\r\n                  <v-list-item-content\r\n                      class=\"align-end\"\r\n                      :class=\"{ 'blue--text': sortBy === key }\"\r\n                  >\r\n                    {{ item[key.toLowerCase()] }}\r\n                  </v-list-item-content>\r\n                </v-list-item>\r\n              </v-list>\r\n            </v-card>\r\n          </v-col>\r\n        </v-row>\r\n      </template>\r\n\r\n      <template v-slot:footer>\r\n        <v-row\r\n            class=\"mt-2\"\r\n            align=\"center\"\r\n            justify=\"center\"\r\n        >\r\n\r\n\r\n          <v-spacer></v-spacer>\r\n\r\n          <span\r\n              class=\"mr-4\r\n            grey--text\"\r\n          >\r\n            第 {{ page }} 到 {{ numberOfPages }}\r\n          </span>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"mr-1\"\r\n              @click=\"formerPage\"\r\n          >\r\n            <v-icon>mdi-chevron-left</v-icon>\r\n          </v-btn>\r\n          <v-btn\r\n              fab\r\n              dark\r\n              color=\"blue darken-3\"\r\n              class=\"ml-1\"\r\n              @click=\"nextPage\"\r\n          >\r\n            <v-icon>mdi-chevron-right</v-icon>\r\n          </v-btn>\r\n        </v-row>\r\n      </template>\r\n    </v-data-iterator>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport {getClassFromApi} from \"../../netwoek/API\";\r\n\r\nexport default {\r\n  data () {\r\n    return {\r\n      search: '',\r\n      filter: {},\r\n\r\n      page: 1,\r\n      itemsPerPage: 12,\r\n      sortBy: 'name',\r\n      keys: [\r\n        'Name',\r\n        'Pid',\r\n\r\n      ],\r\n      items: [\r\n        {\r\n          name: '19计算机应用技术四班',\r\n          pid: 1941054,\r\n        },\r\n        {\r\n          name: '19计算机应用技术三班',\r\n          pid: 1941053,\r\n        },\r\n        {\r\n          name: '19计算机应用技术二班',\r\n          pid: 1941052,\r\n        },\r\n        {\r\n          name: '19计算机应用技术四班1',\r\n          pid: 1941054,\r\n        },\r\n        {\r\n          name: '19计算机应用技术三班1',\r\n          pid: 1941053,\r\n        },\r\n        {\r\n          name: '19计算机应用技术二班1',\r\n          pid: 1941052,\r\n        },\r\n        {\r\n          name: '19计算机应用技术四班2',\r\n          pid: 1941054,\r\n        },\r\n        {\r\n          name: '19计算机应用技术三班2',\r\n          pid: 1941053,\r\n        },\r\n        {\r\n          name: '19计算机应用技术二班2',\r\n          pid: 1941052,\r\n        },\r\n        {\r\n          name: '19计算机应用技术二班3',\r\n          pid: 1941052,\r\n        },\r\n        {\r\n          name: '19计算机应用技术四班4',\r\n          pid: 1941054,\r\n        },\r\n        {\r\n          name: '19计算机应用技术三班4',\r\n          pid: 1941053,\r\n        },\r\n        {\r\n          name: '19计算机应用技术二班5',\r\n          pid: 1941052,\r\n        },\r\n\r\n\r\n      ],\r\n    }\r\n  },\r\n  created() {\r\n    getClassFromApi(this.ClassName.pid)\r\n        .then(res=>{\r\n          this.items=[]\r\n          for(let i=0;i<res.data.list.length;i++) {\r\n            this.$set(this.items.name, i, res.data.list[i].name)\r\n            this.$set(this.items.pid, i, res.data.list[i].pid)\r\n          }\r\n        })\r\n  },\r\n  computed: {\r\n    numberOfPages () {\r\n      return Math.ceil(this.items.length / this.itemsPerPage)\r\n    },\r\n    filteredKeys () {\r\n      return this.keys.filter(key => key !== 'Name')\r\n    },\r\n  },\r\n  methods: {\r\n    goClass(pid){\r\n      this.$router.push('/ClassCr/'+pid)\r\n    },\r\n    nextPage () {\r\n      if (this.page + 1 <= this.numberOfPages) this.page += 1\r\n    },\r\n    formerPage () {\r\n      if (this.page - 1 >= 1) this.page -= 1\r\n    },\r\n\r\n  },\r\n}\r\n</script>\r\n<style scoped>\r\n\r\n</style>"]}]}